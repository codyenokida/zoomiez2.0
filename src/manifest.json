{
  "name": "Zoomiez",
  "version": "1.0.0",
  "description": "Zoomiez",
  "permissions": [
    "storage"
  ],
  "background": { "scripts": ["background/index.js"] },
  "content_scripts": [
    {
      "js": ["content/index.js"],
      "matches": ["https://*/*", "http://*/*"]
    }
  ],
  "browser_action": { "default_popup": "pages/popup/index.html" },
  "icons": { 
    "16": "./images/logo.png",
    "48": "./images/logo.png",
    "128": "./images/logo.png" 
  }
}
